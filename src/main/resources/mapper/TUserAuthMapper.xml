<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jingruan.weighsystem.mapper.TUserAuthMapper">
    <select id="listByUID" resultType="com.jingruan.weighsystem.entity.TUserAuth">
        select * from t_user_auth where uid=#{id};
    </select>

    <insert id="add">
        insert into t_user_auth (uid,aid) values (#{uid},#{aid});
    </insert>

    <delete id="delByUID">
        delete from t_user_auth where uid=#{id};
    </delete>
</mapper>
